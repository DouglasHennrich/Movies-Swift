# Uncomment the next line to define a global platform for your project
platform :ios, '13.0'

# Comment the next line if you don't want to use dynamic frameworks
use_frameworks!

def shared_pods
  pod 'Alamofire'
  pod 'lottie-ios'
  pod 'Kingfisher'
end

target 'Movies' do
  shared_pods
end

target 'MoviesTests' do
  shared_pods
end
